{
  "npm.packageManager": "pnpm",
  "cSpell.words": [
    "accuvote",
    "AGPL",
    "bazelbuild",
    "bazelisk",
    "BINDIR",
    "browserify",
    "buildozer",
    "canonicalization",
    "canonicalize",
    "Canonicalized",
    "canonicalizes",
    "cardless",
    "castability",
    "cvrs",
    "cwd",
    "depset",
    "dotenv",
    "esbuild",
    "evanw",
    "frontends",
    "godotenv",
    "hmpb",
    "imdebug",
    "importpath",
    "jsonl",
    "luxon",
    "Mergeable",
    "Minifier",
    "multitool",
    "mutlitool",
    "OLDPWD",
    "Outbase",
    "overvote",
    "overvoted",
    "overvotes",
    "pagedjs",
    "plustek",
    "pollworker",
    "positionals",
    "previewable",
    "qrcode",
    "scriptname",
    "SEMS",
    "serializable",
    "smartcard",
    "smartcards",
    "stretchr",
    "stylelint",
    "superadmin",
    "testid",
    "testlogs",
    "uncastable",
    "unconfigure",
    "unconfigured",
    "unconfiguring",
    "undervote",
    "undervoted",
    "undervotes",
    "unmount",
    "unprogram",
    "unprogramming",
    "unprograms",
    "usbstick",
    "votingworks",
    "VVSG",
    "vxsweet",
    "yesno"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "explicit",
    "source.fixAll": "explicit"
  },
  "files.watcherExclude": {
    "/**/.cache/bazel/**": true,
    "**/.git/**": true,
    "**/bazel-*/**": true,
    "**/coverage/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    "**/*.tsbuildinfo": true,
    "**/pdf.worker.min.js": true,
    "**/.jestcache": true,
    "**/build": true,
    "**/coverage": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "go.goroot": "${workspaceFolder}/bazel-${workspaceFolderBasename}/external/rules_go~~go_sdk~go_sdk",
  "go.toolsEnvVars": {},
  "go.enableCodeLens": {
    "runtest": false
  },
  "gopls": {
    "build.directoryFilters": [
      "-**/bazel-bin",
      "-**/bazel-out",
      "-**/bazel-testlogs",
      "-**/bazel-${workspaceFolderBasename}",
      "-**/node_modules"
    ],
    "formatting.gofumpt": true,
    "formatting.local": "github.com/kofi-q/${workspaceFolderBasename}",
    "ui.completion.usePlaceholders": false,
    "ui.semanticTokens": true,
    "ui.codelenses": {
      "gc_details": false,
      "regenerate_cgo": false,
      "generate": false,
      "test": false,
      "tidy": false,
      "upgrade_dependency": false,
      "vendor": false
    },
  },
  "go.useLanguageServer": true,
  "go.buildOnSave": "off",
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
  "files.associations": {
    ".bazeliskrc": "shellscript"
  }
}
